{
  "name": "@everglot/landingpage",
  "description": "everglot.com landing page",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-p watch:*",
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "sapper dev",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.pcss -o static/global.css",
    "build:sapper": "sapper build --legacy",
    "build:export": "sapper export --legacy",
    "export": "run-s build:css build:export",
    "migrate": "node-pg-migrate -m src/migrations",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "validate": "svelte-check --ignore src/node_modules/@sapper --compiler-warnings 'module-script-reactive-declaration:ignore'"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "node-pg-migrate": "^5.9.0",
    "pg": "^8.5.1",
    "sirv": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/runtime": "^7.12.13",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.14.27",
    "@types/polka": "^0.5.2",
    "@types/uuid": "^8.3.1",
    "autoprefixer": "^10.2.4",
    "cssnano": "^5.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.4",
    "postcss-cli": "^8.3.1",
    "postcss-load-config": "^3.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.4",
    "rollup": "^2.52.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.1",
    "svelte": "^3.38.2",
    "svelte-check": "^1.1.34",
    "svelte-feather-icons": "^3.3.0",
    "svelte-preprocess": "^4.6.8",
    "tailwindcss": "^2.0.3",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5",
    "uuid": "^8.3.2"
  }
}
